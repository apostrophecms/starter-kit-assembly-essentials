{
  "name": "a3-assembly-boilerplate",
  "version": "1.2.0",
  "description": "A boilerplate for multisite content-management in A3",
  "private": true,
  "scripts": {
    "build": "APOS_UPLOADFS_ASSETS=1 NODE_ENV=production bash -c 'node app @apostrophecms/asset:build --site=dashboard && ./scripts/for-each-theme @apostrophecms/asset:build'",
    "start": "APOS_UPLOADFS_ASSETS=1 NODE_ENV=production node app",
    "dev": "(ENV=dev node app @apostrophecms/asset:build --site=dashboard && ENV=dev ./scripts/for-each-theme @apostrophecms/asset:build) && ENV=dev node app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apostrophecms/a3-assembly-boilerplate"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Apostrophe Technologies",
  "license": "UNLICENSED",
  "dependencies": {
    "@apostrophecms-pro/basics": "^1.0.0-alpha.3.1",
    "@apostrophecms-pro/multisite": "^3.0.0-alpha.8",
    "@apostrophecms-pro/palette": "^3.0.0-alpha.6",
    "apostrophe": "^3.0.0",
    "node-fetch": "^2.6.1",
    "qs": "^6.9.6",
    "require-all": "^3.0.0"
  },
  "devDependencies": {
    "stylelint-config-punkave": "^1.1.3"
  }
}
