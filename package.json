{
  "name": "a3-assembly-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for multisite content-management in A3",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "start": "node app",
    "dev": "(ENV=dev node app @apostrophecms/asset:build --site=dashboard && ENV=dev node app @apostrophecms/asset:build --temporary-site --theme=default) && ENV=dev webpack serve & ENV=dev node app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apostrophecms/a3-assembly-boilerplate"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Apostrophe Technologies",
  "license": "UNLICENSED",
  "dependencies": {
    "@apostrophecms-pro/basics": "github:apostrophecms/basics",
    "@apostrophecms-pro/multisite": "github:apostrophecms/multisite",
    "@apostrophecms-pro/palette": "github:apostrophecms/palette",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "apostrophe": "github:apostrophecms/apostrophe#3.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^3.5.1",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.4",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.11.0",
    "require-all": "^3.0.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0"
  },
  "devDependencies": {
    "webpack-dev-server": "^3.11.2"
  }
}
